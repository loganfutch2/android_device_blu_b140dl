# Mount point		FStype					Device													Flags

/cache        ext4        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/cache         /dev/block/mmcblk0p37     flags=display="Cache";canbewiped;wipeingui;wipeduringfactoryreset

/data         ext4        /dev/block/dm-3      /dev/block/mmcblk0p39  flags=length=-16384;display="Data";backup=1;canbewiped;wipeingui;wipeduringfactoryreset;settingsstorage;encryptable=/dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/metadata

/boot         emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/boot          /dev/block/mmcblk0p29      flags=display="Boot";backup=1;flashimg=1
/logo         emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/logo          /dev/block/mmcblk0p31      flags=display="Logo";backup=1;flashimg=1
/nvram        emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/nvram         /dev/block/mmcblk0p26      flags=display="NvRAM";backup=1;flashimg=1
/nvdata       ext4        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/nvdata        /dev/block/mmcblk0p9     flags=display="NvData";backup=1;flashimg=1

/recovery     emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/recovery      /dev/block/mmcblk0p2      flags=display="Recovery";backup=1;flashimg=1
/metadata     ext4        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/metadata      /dev/block/mmcblk0p10

/external_sd  auto        /dev/block/mmcblk1p1           /dev/block/mmcblk1                     flags=display="Micro SDcard";storage;wipeingui;removable;settingsstorage;backup=0;encryptable=userdata

/usbotg       auto        /dev/block/sda1                /dev/block/sda                         flags=display="USB-OTG";backup=0;wipeingui;storage;removable

auto          vfat        /devices/mtk-msdc.0/11230000.msdc0*                   defaults        voldmanaged=sdcard0:auto
auto          auto        /devices/mtk-msdc.0/11240000.msdc1*                   defaults        voldmanaged=sdcard1:auto;noemulatedsd;encryptable=userdata 
/devices/platform/mt_usb* auto vfat defaults voldmanaged=usbotg:auto

/lk           emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/lk            /dev/block/mmcblk0p27      flags=display="Uboot";backup=1;flashimg=1
/proinfo      emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/proinfo       /dev/block/mmcblk0p17      flags=display="Proinfo";backup=1
/protect_f    ext4        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/protect1      /dev/block/mmcblk0p12      flags=display="Protect_F";backup=1
/protect_s    ext4        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/protect2      /dev/block/mmcblk0p13      flags=display="Protect_S";backup=1
/misc         emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/para          /dev/block/mmcblk0p3      flags=display="Parametr";backup=1
/tee1         emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/tee1          /dev/block/mmcblk0p33     flags=display="Tee1";backup=1;flashimg=1
/tee2         emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/tee2          /dev/block/mmcblk0p34     flags=display="Tee2";backup=1;flashimg=1
/persistent   emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/frp           /dev/block/mmcblk0p7     flags=display="Frp";backup=1
/expdb        emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/expdb         /dev/block/mmcblk0p4
/seccfg       emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/seccfg        /dev/block/mmcblk0p14

